# dIKtate Environment Configuration
# Copy this file to .env and fill in your values

# Processing Mode: "local", "gemini", "anthropic", or "openai"
PROCESSING_MODE=local

# Gemini API (only required if PROCESSING_MODE=gemini)
GEMINI_API_KEY=your-gemini-api-key-here

# Anthropic API (only required if PROCESSING_MODE=anthropic)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# OpenAI API (only required if PROCESSING_MODE=openai)
OPENAI_API_KEY=your-openai-api-key-here

# Debug Mode (enables StreamHandler logging)
DEBUG=0
