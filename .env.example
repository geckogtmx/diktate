# dIKtate Environment Configuration
# Copy this file to .env and fill in your values

# Processing Mode: "local" (Ollama) or "cloud" (Gemini API)
PROCESSING_MODE=local

# Gemini API (only required if PROCESSING_MODE=cloud)
GEMINI_API_KEY=your-gemini-api-key-here

# Debug Mode (enables StreamHandler logging)
DEBUG=0
